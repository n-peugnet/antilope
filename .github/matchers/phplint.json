{
    "problemMatcher": [
        {
            "owner": "phplint",
            "fileLocation": "relative",
            "pattern": [
                {
                    "regexp": "^(.+)\\sin\\s(\\./.+\\.php)\\son\\sline\\s(\\d+)$",
                    "message": 1,
                    "file": 2,
                    "line": 3
                }
            ]
        }
    ]
}
